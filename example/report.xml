    <reportHeader height="250" isVisible="1" backgroundColor="#FFF">
        <textField format="text" posX="60" posY="0" height="5" width="60" border="1">reportHeader textField1</textField>
        <textField format="text" posX="60" posY="5" height="5" width="60" border="1">reportHeader textField2</textField>
        <textZone format="text" posX="60" posY="10" height="40" width="60" border="1">reportHeader textZone</textZone>
