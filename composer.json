{
    "name": "rev42/RevPDFLib",
    "type": "library",
    "description": "PDF generating library",
    "keywords": ["revpdf", "pdf", "php"],
    "homepage": "http://www.revpdf.org",
    "version": "2.0.1-dev",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Olivier Cornu",
            "email": "contact@revpdf.org"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jwage/EasyCSV",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/jwage/EasyCSV.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rev42/tfpdf" : "1.0",
        "jwage/EasyCSV": "dev-master",
        "symfony/http-kernel": "2.1.0-BETA1",
        "symfony/event-dispatcher": "2.1.0-BETA1",
        "symfony/dependency-injection": "2.1.0-BETA1",
        "symfony/config":"2.1.0-BETA1"
    },
   "autoload": {
        "psr-0": {
            "RevPDFLib": "src/",
            "EasyCSV": "vendor/EasyCSV/lib/",
            "tFPDF": "vendor/tfpdf/src/"
        }
    }
}
