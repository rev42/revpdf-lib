{
    "name": "rev42/RevPDFLib",
    "type": "library",
    "description": "The PHP micro-framework based on the Symfony2 Components",
    "keywords": ["revpdf", "pdf", "php"],
    "homepage": "http://www.revpdf.org",
    "version": "2.0.0",
    "license": "GPL",
    "authors": [
        {
            "name": "Olivier Cornu",
            "email": "contact@revpdf.org"
        }
    ],
    "repositories": {
        "jwageEasyCSV": {
            "package": {
                "name": "EasyCSV",
                "version": "master",
                "source": {
                    "url": "https://github.com/jwage/EasyCSV.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/http-kernel": "2.1.*",
        "symfony/event-dispatcher": "2.1.*",
        "symfony/dependency-injection": "2.1.*",
        "symfony/config":"2.1.*",
        "EasyCSV": "master"
    },
    "autoload": {
        "psr-0": {
            "RevPDFLib": "src/",
            "EasyCSV": "vendor/EasyCSV/lib/"
        }
    }
}