    "description": "PDF generating library",
    "keywords": ["revpdf", "pdf", "php"],
    "homepage": "http://www.revpdf.org",
    "version": "2.1.1-dev",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Olivier Cornu",
            "email": "contact@revpdf.org"
        }
    ],
    "require": {
        "php": ">=5.3",
        "rev42/tfpdf" : "1.0",
        "jwage/easy-csv": "dev-master",
        "symfony/http-kernel": "~2.2",
        "symfony/event-dispatcher": "~2.2",
        "symfony/dependency-injection": "~2.2",
        "symfony/config":"~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.19",
        "phpunit/dbunit": "1.2.3",
        "phpmd/phpmd": "1.5.0"
