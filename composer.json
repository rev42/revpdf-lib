{
    "name": "rev42/RevPDFLib",
    "type": "library",
    "description": "The PHP micro-framework based on the Symfony2 Components",
    "keywords": ["revpdf", "pdf", "php"],
    "homepage": "http://www.revpdf.org",
    "version": "2.0.0",
    "license": "GPL",
    "authors": [
        {
            "name": "Olivier Cornu",
            "email": "contact@revpdf.org"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "EasyCSV",
                "version": "master",
                "source": {
                    "url": "https://github.com/jwage/EasyCSV.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "tcpdf",
                "version": "master",
                "source": {
                    "url": "git://tcpdf.git.sourceforge.net/gitroot/tcpdf/tcpdf",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "tfpdf",
                "version": "master",
                "source": {
                    "url": "https://github.com/rev42/tFPDF.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "tfpdf" : "master",
        "EasyCSV": "master",
        "symfony/http-kernel": "2.0.7",
        "symfony/event-dispatcher": "2.0.7",
        "symfony/dependency-injection": "2.0.7",
        "symfony/config":"2.0.7"
    },
   "autoload": {
        "psr-0": {
            "RevPDFLib": "src/",
            "EasyCSV": "vendor/EasyCSV/lib/",
            "tFPDF": "vendor/tfpdf/src/"
        }
    }
}